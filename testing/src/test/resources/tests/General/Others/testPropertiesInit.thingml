import "../../thingml.thingml"

thing TestPropertiesInit includes Test
@test "x # AABBCCDDEEFF"
{
    property A1 : Char = 'A'
    property A2 : Char = A1
    
	readonly property B1 : Char = 'B'
    readonly property B2 : Char = B1

    property C1 : Char = 'Z'
    property C2 : Char = C1
    
	readonly property D1 : Char = 'Z'
    readonly property D2 : Char = D1

	property E1 : Integer = 5
	property E2 : Integer = E1

	readonly property F1 : Integer = 5
	readonly property F2 : Integer = F1
	

	statechart Chart init INIT {
    
    	state INIT {
			on entry do
				TestOut(A1)
				TestOut(A2)

				TestOut(B1)
				TestOut(B2)

				TestOut(C1)
				TestOut(C2)

				TestOut(D1)
				TestOut(D2)

				if(E1 == 10) do
					TestOut('E')
				end
				if(E2 == 10) do
					TestOut('E')
				end

				if(F1 == 10) do
					TestOut('F')
				end
				if(F2 == 10) do
					TestOut('F')
				end
				
				TestDone()
			end
    	}
    }
}

configuration conf {
	instance test : TestPropertiesInit
	set test.C1 = 'C'
	set test.D1 = 'D'
	set test.E1 = 5 + 5
	set test.F1 = 5 + 5
}