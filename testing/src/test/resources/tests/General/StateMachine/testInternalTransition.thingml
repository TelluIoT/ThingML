import "../../thingml.thingml"

thing TestInternalTransition includes Test 
@test "i # EI(X)?"
@test "t # EXTE(X)?"
@test "titi # EXTEIXTEI(X)?"
{
	statechart TestInternalTransition init I {
		state I {
			on entry TestOut('E')
			on exit  TestOut('X')
			
			internal
			event m : Test?In
			guard m.c == 'i'
			action TestOut('I')
			
			transition -> I
			event m : Test?In
			guard m.c == 't'
			action TestOut('T')
			
			internal event Test?Done
			action TestDone()
		}
	}
}
