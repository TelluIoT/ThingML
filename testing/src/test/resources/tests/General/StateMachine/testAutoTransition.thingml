import "../../thingml.thingml"

thing TestAutoTransition includes Test 
@test "ttt # IJJKKK"
{
	statechart TestAutoTransition init I {
		state I {
			on entry TestOut('I')
			
			transition -> J
			action TestOut('J')
			
			transition -> K
			event m : Test?In
			guard m.c == 't'
		}
		state J {
			on entry TestOut('J')
			
			transition -> K
			event m : Test?In
			guard m.c == 't'
		}
		state K {
			on entry TestOut('K')
			
			transition -> K
			event m : Test?In
			guard m.c == 't'
			
			internal event Test?Done
			action TestDone()
		}
	}
}
