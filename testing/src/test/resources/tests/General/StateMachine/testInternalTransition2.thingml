import "../../thingml.thingml"

thing TestInternalTransition2 includes Test 
@test "i # EI"
@test "t # ET"
@test "titi # ETITI"
{
	statechart TestInternalTransition2 init I {	
		on entry TestOut('E')
		
		internal event Test?Done
		action TestDone()
			
		internal
		event m : Test?In
		guard m.c == 'i'
		action TestOut('I')
			
		internal
		event m : Test?In
		guard m.c == 't'
		action TestOut('T')	
	
		state I {
			
		}
	}
}
